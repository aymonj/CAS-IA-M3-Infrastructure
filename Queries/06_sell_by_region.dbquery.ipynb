   "source": [
    "SELECT\n",
    "        a.StateProvince         as region,\n",
    "        COUNT(soh.SalesOrderID) AS NombreCommandes\n",
    "FROM\n",
    "        saleslt.Address a\n",
    "JOIN\n",
    "        saleslt.SalesOrderHeader soh\n",
    "\tON\n",
    "\t\t\ta.AddressID = soh.BillToAddressID\n",
    "GROUP BY\n",
    "        a.StateProvince\n",
    "ORDER BY\n",
    "        a.StateProvince;"
   ]
  }
